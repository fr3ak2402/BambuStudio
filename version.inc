# Included by CMakeLists, edited by the build script
# (the version numbers are generated by the build script from the git current label)

set(SLIC3R_APP_NAME "GalaxySlicerNeo")
set(SLIC3R_APP_KEY "GalaxySlicerNeo")

if(NOT DEFINED BBL_RELEASE_TO_PUBLIC)
set(BBL_RELEASE_TO_PUBLIC "1")
endif()
if(NOT DEFINED BBL_INTERNAL_TESTING)
set(BBL_INTERNAL_TESTING "0")
endif()

# The build_version should start from 50 in master branch

set(SLIC3R_VERSION "01.10.01.50")

string(REPLACE "." "," SLIC3R_COMMA_SEPARATED_VERSION ${SLIC3R_VERSION})
set(SLIC3R_COMMA_SEPARATED_VERSION "${SLIC3R_COMMA_SEPARATED_VERSION}")

# GalaxySlicerNeo version
set(GSN_VERSION "V1.0.8")

# Build number
string(TIMESTAMP BUILD_NUM "%Y.%j" UTC)
set(GSN_BUILD ${BUILD_NUM})
